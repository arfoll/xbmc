<playercorefactory>
  <players>
    <!-- These are compiled-in as re-ordering them would break scripts
    The following aliases may also be used:
      audiodefaultplayer, videodefaultplayer, videodefaultdvdplayer
    <player name="DVDPlayer" audio="true" video="true" />
    <player name="DVDPlayer" /> placeholder for MPlayer
    <player name="PAPlayer" audio="true" />
    -->
    <player name="meegovideoplayer" type="meegoplayer" audio="false" video="true">  
      <pinkvideo>true</pinkvideo>  
      <pinkmusic>false</pinkmusic>  
    </player>  
 
    <player name="meegoaudioplayer" type="meegoplayer" audio="true" video="false">  
      <pinkvideo>false</pinkvideo>  
      <pinkmusic>true</pinkmusic>  
    </player> 
  </players>

  <rules name="system rules">
    <rule video="true"  player="meegovideoplayer"/>  
    <rule audio="true"  player="meegoaudioplayer"/>  
  </rules>
</playercorefactory>
